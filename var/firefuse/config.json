{
  "FireREST": {
    "title": "FireREST Reference Service",
    "provider": "Demo",
    "version": {
      "major": "0",
      "minor": "11",
      "patch": "0"
    }
  },
  "background-worker": {
    "idle-period": 900
  },
  "cv": {
    "cve_map": {
      "calc-offset": {
        "firesight": [
          {
            "op": "calcOffset",
            "name": "model",
            "minval": "{{threshold}}",
            "roi": "{{roi}}",
            "xtol": "{{xtol}}",
            "ytol": "{{ytol}}",
            "channels": [],
            "template": "{{saved}}"
          },
          {
            "op": "drawRects",
            "model": "model",
            "color": "{{rectColor}}"
          }
        ],
        "properties": {
          "roi": [
            350,
            50,
            100,
            100
          ],
          "rectColor": [
            32,
            255,
            32
          ],
          "xtol": 32,
          "ytol": 32,
          "threshold": 0.7
        }
      },
      "find-crash-dummy": {
        "firesight": [
          {
            "op": "matchTemplate",
            "name": "fiducial",
            "corr": "{{corr}}",
            "template": "{{img}}",
            "threshold": "{{threshold}}"
          },
          {
            "op": "drawRects",
            "model": "fiducial",
            "color": "{{rectColor}}"
          }
        ],
        "properties": {
          "corr": 0.95,
          "threshold": 0.8,
          "rectColor": [
            32,
            255,
            32
          ],
          "img": "/home/pi/FireREST/www/img/bwwb.png"
        }
      },
      "locate-part": {
        "firesight": [
          {
            "op": "absdiff",
            "path": "{{saved}}"
          },
          {
            "op": "threshold",
            "type": "{{type}}",
            "thresh": "{{thresh}}",
            "maxval": "{{maxval}}"
          },
          {
            "op": "morph",
            "ksize": "{{ksize}}",
            "mop": "{{mop}}",
            "iterations": "{{iterations}}",
            "shape": "{{shape}}"
          },
          {
            "op": "minAreaRect",
            "name": "singleBlob",
            "min": "{{min}}",
            "max": "{{max}}",
            "channel": "{{channel}}"
          },
          {
            "op": "stageImage",
            "stage": "input"
          },
          {
            "op": "drawRects",
            "model": "singleBlob",
            "thickness": "{{thickness}}",
            "color": "{{rectColor}}"
          }
        ],
        "properties": {
          "rectColor": [
            32,
            255,
            32
          ]
        }
      }
    },
    "camera_map": {
      "1": {
        "width": 800,
        "height": 200,
        "profile_map": {
          "gray": {
            "cve_names": [
              "calc-offset",
              "find-crash-dummy",
              "locate-part"
            ]
          },
          "bgr": {
            "cve_names": [
              "calc-offset",
              "find-crash-dummy",
              "locate-part"
            ]
          }
        }
      }
    }
  },
